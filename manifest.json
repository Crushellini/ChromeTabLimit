{
  "short_name": "React App",
  "name": "Chrome Tab Limit",
  "manifest_version": 2,
  "start_url": "./index.html",
  "version": "1.0",
  "display": "standalone",
  "permissions": [ "storage", "alarms", "tabs" ],
  "browser_action": {
    "default_popup" : "popup.html"
  },
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "theme_color": "#008B8B"
}
