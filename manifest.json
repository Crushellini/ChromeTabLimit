{
  "short_name": "Tab Limit",
  "name": "Chrome Tab Limit",
  "manifest_version": 2,
  "start_url": "./index.html",
  "version": "1.0",
  "display": "standalone",
  "permissions": [ "storage", "alarms", "tabs" ],
  "icons": { "128": "ChromeTabLimitIcon128.png" },
  "browser_action": {
    "default_popup" : "./popup/popup.html",
    "default_icon" : "./popup/popupIcon32.png"
  },
  "background": {
    "scripts": [ "./background/background.js", "./popup/popup.js"],
    "persistent": true
  },
  "theme_color": "#008B8B"
}
